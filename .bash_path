# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/.bin" ]
then
	export PATH="$HOME/.bin:$PATH"
fi

if [ -x "$(command -v ruby)" ]
then
	# add gems to path
	PATH=$(ruby -e "puts Gem.user_dir")/bin:$PATH
fi

export EDITOR=nano
export DIFFPROG=meld
export SVN_MERGE=/home/mpdeimos/.bin/svn-merge
